# See http://help.github.com/ignore-files/ for more about ignoring files.

# Exclude build artifacts
/dist/
/tmp/
/out-tsc/
/bazel-out/

# Exclude generated files during development/build
/src/**/*.js
/src/**/*.js.map
/src/**/*.d.ts
/src/**/*.metadata.json
/e2e/*.js
/e2e/*.js.map
/e2e/*.d.ts

# Exclude npm and Yarn related files
/node_modules/
npm-debug.log
yarn-error.log
yarn.lock

# Exclude IDE and editor-specific files
.idea/
.project
.classpath
.c9/
*.launch
.settings/
*.sublime-workspace

# Visual Studio Code
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json

# Angular CLI build artifacts
.angular/cache/
.history/

# Test and coverage reports
/coverage/
/testem.log

# Typings (if not used in your project)
/typings/

# System-specific files
.DS_Store
Thumbs.db

# Environment files (if not needed in your repository)
/src/environments/environment.ts
